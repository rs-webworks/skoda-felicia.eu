<div class="hidden-xs">
    {{ form_start(searchForm, {'attr': {'class': "navbar-form navbar-right"}}) }}

    <div class="form-group">
        <input type="text" name="{{ searchForm.query.vars.full_name }}" class="form-control"
               placeholder="{{ searchForm.query.vars.attr.placeholder }}">
    </div>
    {% do searchForm.query.setRendered %}

    <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>

    {{ form_end(searchForm) }}
</div>